{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": "defaults"
				},
				"modules": false
			}
		],
		"@babel/preset-react"
	],
	"plugins": ["@babel/proposal-export-default-from"],
	"env": {
		"cjs": {
			"presets": [
				[
					"@babel/preset-env",
					{
						"targets": {
							"node": true
						}
					}
				],
				"@babel/preset-react"
			]
		}
	}
}
